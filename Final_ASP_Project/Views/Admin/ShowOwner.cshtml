@model IEnumerable<ApplicationUser>



    @{
  
    Layout = "~/Views/Shared/_LayoutAdmin.cshtml";
}


<div class="card">
    <div class="card-body">
        <h1 class="card-title" style="text-align:center; font-size:30px">Owner</h1>

        <a class="btn btn-primary" asp-action="RegisterAccOwner"><h3>Create Owner New</h3></a>
        <table class="table">
            <thead>
                <tr>
                    
                    <th>
                        User Name
                    </th>
                   
                    <th>
                       Email
                    </th>
                    
                    <th>Address</th>
                    <th>Gender</th>
                    <th>

                    </th>
                </tr>
            </thead>
            <tbody>
                @foreach (var user in Model)
                {
                    <tr>
                      
                        <td>@user.FullName</td>
                        <td>@user.Email</td>
                    
                       
                        <td>@user.Address</td>
                        <td>Gender</td>
                        @*<td>
                            <a class="btn btn-primary mdi mdi-pencil" asp-controller="Admin" asp-action="EditOwner" asp-route-id="@user.Id">Edit</a>
                        </td>*@
                    </tr>
                }
            </tbody>
        </table>
    </div>
</div>

